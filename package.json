{
  "name": "employee-safety-client",
  "version": "2.0.9",
  "description": "Employee Safety Client - Comprehensive Employee Security Solution",
  "main": "electron/main-minimal.js",
  "bin": {
    "employee-safety": "dist/main/cli.js"
  },
  "scripts": {
    "clean": "npx rimraf dist",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run compile",
    "build:native:mac": "cd native-event-monitor && npm install && npm run build && npx electron-rebuild --version=$(npx electron --version | cut -d'v' -f2)",
    "build:native:win": "cd native-event-monitor-win && npm install && npm run build",
    "build:mac": "npm run clean && npm run compile && npm run build:native:mac",
    "build:win": "npm run clean && npm run compile && npm run build:native:win",
    "electron": "npm run compile && npx electron --expose-gc electron/main-minimal.js",
    "pack:mac": "npm run build:mac && npm run pack:mac:universal",
    "pack:mac:universal": "node scripts/pack-mac-universal.js",
    "pack:mac:dmg": "bash scripts/create-dmg.sh",
    "pack:mac:pkg": "bash scripts/create-pkg.sh",
    "pack:mac:zip": "npm run build:mac && npx electron-builder --mac zip --config electron-builder.yml",
    "pack:mac:all": "npm run build:mac && npx electron-builder --mac --config electron-builder.yml",
    "pack:mac:builder": "npm run build:mac && npx electron-builder --mac --config electron-builder.yml",
    "pack:win": "node scripts/build-windows.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:performance": "ts-node test/performance/benchmark.ts",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:active-win": "jest --testPathPattern=active-win-compat.test.ts",
    "test:window-monitoring": "jest --testPathPattern=window-monitoring.test.ts",
    "benchmark:active-win": "npm run compile && node --expose-gc dist/test/performance/active-win-compat-benchmark.js",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "check-permissions": "node dist/main/cli.js check-permissions",
    "open-accessibility-settings": "open 'x-apple.systempreferences:com.apple.preference.security?Privacy_Accessibility'",
    "verify-packaging": "bash scripts/verify-packaging.sh",
    "stats": "node dist/main/cli.js stats",
    "stats:reset": "node dist/main/cli.js stats --reset",
    "stats:json": "node dist/main/cli.js stats --json",
    "verify:bundle-id": "npx ts-node scripts/verify-bundle-id.ts",
    "prebuild": "npm run verify:bundle-id"
  },
  "keywords": [
    "employee-monitoring",
    "desktop-application",
    "typescript",
    "cross-platform",
    "fsm"
  ],
  "author": "Employee Monitoring Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "debug": "^4.4.3",
    "electron-log": "^5.4.3",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "eventemitter3": "^4.0.7",
    "node-machine-id": "^1.1.12",
    "screenshot-desktop": "^1.12.7",
    "sharp": "^0.34.4",
    "socket.io-client": "^4.8.1",
    "systeminformation": "^5.17.12",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@electron/windows-sign": "^1.1.2",
    "@types/electron": "^1.4.38",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.0.0",
    "electron": "^28.2.10",
    "electron-builder": "^25.0.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.0",
    "filename-reserved-regex": "^4.0.0",
    "filenamify": "^7.0.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.20",
    "postject": "1.0.0-alpha.6",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "win32",
    "darwin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangxiaoyu2000/employee-s.git"
  },
  "bugs": {
    "url": "https://github.com/zhangxiaoyu2000/employee-s/issues"
  },
  "homepage": "https://github.com/zhangxiaoyu2000/employee-s#readme",
  "build": {
    "extends": null
  }
}
