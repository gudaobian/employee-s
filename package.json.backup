{
  "name": "employee-monitoring-client-new",
  "version": "1.0.0",
  "description": "Employee Monitoring Client - New Three-Layer Architecture",
  "main": "electron/main-minimal.js",
  "bin": {
    "employee-monitor-new": "dist/main/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run clean && npm run compile",
    "build:native:mac": "cd native-event-monitor && npm install && npm run build && npx electron-rebuild --version=$(npx electron --version | cut -d'v' -f2)",
    "build:native:win": "cd native-event-monitor-win && npm install && npm run build",
    "build:mac": "npm run clean && npm run compile && npm run build:native:mac",
    "build:win": "npm run clean && npm run compile && npm run build:native:win",
    "electron": "npm run compile && npx electron electron/main-minimal.js",
    "pack:mac": "npm run build:mac && npx electron-packager . EmployeeMonitor --platform=darwin --arch=x64 --out=release --overwrite --icon=assets/icons/icon.icns --no-prune",
    "pack:mac:smart": "node scripts/smart-pack.js",
    "pack:win": "node scripts/build-windows.js",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "keywords": [
    "employee-monitoring",
    "desktop-application",
    "typescript",
    "cross-platform",
    "fsm"
  ],
  "author": "Employee Monitoring Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "eventemitter3": "^4.0.7",
    "screenshot-desktop": "^1.12.7",
    "socket.io-client": "^4.8.1",
    "systeminformation": "^5.17.12",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@electron/windows-sign": "^1.1.2",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^8.0.0",
    "electron": "^25.9.0",
    "electron-builder": "^25.0.0",
    "electron-packager": "^17.1.2",
    "eslint": "^8.0.0",
    "filename-reserved-regex": "^4.0.0",
    "filenamify": "^7.0.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.20",
    "postject": "1.0.0-alpha.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "win32",
    "darwin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangxiaoyu2000/employee-s.git"
  },
  "bugs": {
    "url": "https://github.com/zhangxiaoyu2000/employee-s/issues"
  },
  "homepage": "https://github.com/zhangxiaoyu2000/employee-s#readme",
  "build": {
    "appId": "com.company.employee-monitor",
    "productName": "EmployeeMonitor",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "assets/**/*",
      "native-event-monitor/build/Release/*.node",
      "native-event-monitor-win/build/Release/*.node",
      "!**/*.ts",
      "!**/*.map",
      "!electron/**/*.ts"
    ],
    "extraMetadata": {
      "main": "electron/main-minimal.js"
    },
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icons/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "assets/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "assets/icons/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}